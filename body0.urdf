<robot name = "robot">
    <link name="Torso">
        <inertial>
            <origin xyz="0 0 0.5" rpy="0 0 0"/>
            <mass value="1" />
            <inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100" />
        </inertial>
        <visual>
            <origin xyz="0 0 0.5" rpy="0 0 0"/>
            <geometry>
                <box size="1 0.5 0.5" />
            </geometry>
            <material name="Black">
                <color rgba="0 0 0 1.0"/>
            </material>
        </visual>
